			<group text="groups.signal" expand="true">
				<grid rows="3" cols="2" hspacing="4">
					<knob id="g_in" />
					<vbox vfill="false">
						<label text="labels.input" halign="0" />
						<value id="g_in" same_line="true" halign="0" />
					</vbox>
					
					<knob id="g_out" />
					<vbox vfill="false">
						<label text="labels.output" expand="true" halign="0" />
						<value id="g_out" same_line="true" halign="0" />
					</vbox>
					
					<knob id="bal" balance="0.5" scale_color="yellow" />
					<vbox vfill="false">
						<label text="labels.balance" halign="0" />
						<value id="bal" same_line="true" halign="0" />
					</vbox>
				</grid>
			</group>
			<group text="groups.analysis" expand="true">
				<vbox>
					<label text="labels.fft.fft" halign="0" />
					<combo id="fft" fill="true" />
					
					<grid rows="2" cols="2" transpose="false" expand="true" hspacing="4" >
						<knob id="react" size="20" />
						<vbox vfill="false">
							<label text="labels.metering.reactivity" halign="0" />
							<value id="react" same_line="true" halign="0" />
						</vbox>
						
						<knob id="shift" size="20" />
						<vbox vfill="false">
							<label text="labels.metering.shift" halign="0" />
							<value id="shift" same_line="true" halign="0" />
						</vbox>
					</grid>
				</vbox>
			</group>
		</vbox>
		
		<cell cols="3">
			<cgroup id="fsel">
				<ui:for id="blk" first="0" last="3">
					<ui:eval id="chc" value="(:blk mod 2) ? 'right_channel' : 'left_channel'" />
					<ui:eval id="chp" value="(:blk mod 2) ? 'r' : 'l'" />
					<ui:eval id="f" value=":blk idiv 2" />
					
					<grid rows="5" cols="17">
						<label text="labels.units.hz" fill="true" halign="0" />
						<!-- x16 frequencies -->
						<ui:if test=":num_channels ile 16">
							<label text="16" />
							<label text="25" />
							<label text="40" />
							<label text="63" />
							<label text="100" />
							<label text="160" />
							<label text="250" />
							<label text="400" />
							<label text="630" />
							<ui:attributes text="labels.values.x_k">
								<label text:value="1" />
								<label text:value="1.6" />
								<label text:value="2.5" />
								<label text:value="4" />
								<label text:value="6.3" />
								<label text:value="10" />
								<label text:value="16" />
							</ui:attributes>
						</ui:if>
						<!-- x32 frequencies (part 1) -->
						<ui:if test="(:num_filters igt 16) and (:f ieq 0)">
							<label text="16" />
							<label text="20" />
							<label text="25" />
							<label text="31.5" />
							<label text="40" />
							<label text="50" />
							<label text="63" />
							<label text="80" />
							<label text="100" />
							<label text="125" />
							<label text="160" />
							<label text="200" />
							<label text="250" />
							<label text="315" />
							<label text="400" />
							<label text="500" />
						</ui:if>
						<!-- x32 frequencies (part 2) -->
						<ui:if test="(:num_filters igt 16) and (:f ieq 1)">
							<label text="630" />
							<label text="800" />
							<ui:attributes text="labels.values.x_k">
								<label text:value="1" />
								<label text:value="1.25" />
								<label text:value="1.6" />
								<label text:value="2" />
								<label text:value="2.5" />
								<label text:value="3.15" />
								<label text:value="4" />
								<label text:value="5" />
								<label text:value="6.3" />
								<label text:value="8" />
								<label text:value="10" />
								<label text:value="12.5" />
								<label text:value="16" />
								<label text:value="20" />
							</ui:attributes>
						</ui:if>
						
						<label text="labels.ctl" halign="0" />
						<ui:for id="i" first=":f*16" last=":f*16+15">
							<ui:set id="ch" value="${chp}_${i}" />
							<grid rows="3" cols="2" fill="false">
								<button id="xe${ch}" led="true" size="12" color="yellow" />
								<label text="labels.chan.on" font_size="9" />
								<button id="xs${ch}" led="true" size="12" color="green" />
								<button id="xm${ch}" led="true" size="12" color="red" />
								<label text="labels.chan.S_olo" font_size="9" />
								<label text="labels.chan.M_ute" font_size="9" />
							</grid>
						</ui:for>
						
						<cell rows="2"><label text="labels.gain:db" halign="0" /></cell>
						<ui:for id="i" first=":f*16" last=":f*16+15">
							<knob id="g${chp}_${i}"  size="20" scolor="${chc}" />
						</ui:for>
						
						<ui:for id="i" first=":f*16" last=":f*16+15">
							<value id="g${chp}_${i}" detailed="false" />
						</ui:for>
					</grid>
				</ui:for>
			</cgroup>
		</cell>		
	</grid>
</plugin>
