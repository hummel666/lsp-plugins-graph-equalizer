<plugin resizable="true">
	<ui:eval id="num_filters" value="(ex :gm_16) ? 32 : 16" />
	<ui:eval id="num_blocks"  value="(ex :gm_16) ? 4 : 2" />
	
	<grid rows="2" cols="3" spacing="4">
		<vbox spacing="4" expand="true">
			<!-- Signal meters -->
			<hbox spacing="4">
				<ledmeter hexpand="true" angle="0">
					<ledchannel id="iml" min="-36 db" max="+6 db" log="true" type="rms_peak"
						peak.visibility="true" 
						value.color="(:lstn ieq 1) ? 'mid_channel' : 'left_channel'" />
					<ledchannel id="imr" min="-36 db" max="+6 db" log="true" type="rms_peak"
						peak.visibility="true" 
						value.color="(:lstn ieq 1) ? 'side_channel' : 'right_channel'" />
				</ledmeter>
				<label text="labels.input:db" />
				<void min_width="64" />
				<label text="labels.output:db" />
				<ledmeter hexpand="true" angle="0">
					<ledchannel id="sml" min="-36 db" max="+6 db" log="true" type="rms_peak"
						peak.visibility="true" 
						value.color="(:lstn ieq 1) ? 'mid_channel' : 'left_channel'" />
					<ledchannel id="smr" min="-36 db" max="+6 db" log="true" type="rms_peak"
						peak.visibility="true" 
						value.color="(:lstn ieq 1) ? 'side_channel' : 'right_channel'" />
				</ledmeter>
			</hbox>
			
			<!-- Graph -->
			<graph width.min="640" height.min="340" expand="true" fill="true">
				<origin hpos="-1" vpos="-1" visible="false" />
				
				<!-- Gray grid -->
				<ui:attributes ox="0" oy="1" color="graph_alt_4">
					<ui:for id="f" first="20" last="90" step="10">
						<marker v="${f}" />
					</ui:for>
					<ui:for id="f" first="200" last="900" step="100">
						<marker v="${f}" />
					</ui:for>
					<ui:for id="f" first="2000" last="9000" step="1000">
						<marker v="${f}" />
					</ui:for>
					<marker value="20000" />
				</ui:attributes>
				<ui:for id="g" first="-36" last="36" step="24">
					<marker v="${g} db" ox="1" oy="0" color="graph_alt_4" />
				</ui:for>
				<ui:for id="g" first="-18" last="18" step="12">
					<marker v="${g} db" ox="1" oy="0" color="graph_alt_4" visibility=":zoom le -18 db" />
				</ui:for>
				<ui:for id="g" first="-9" last="9" step="6">
					<marker v="${g} db" ox="1" oy="0" color="graph_alt_4" visibility=":zoom le -33 db" />
				</ui:for>
				
				<!-- Yellow grid -->
				<ui:attributes ox="0" oy="1" color="graph_alt_2">
					<marker v="100" />
					<marker v="1000" />
					<marker v="10000" />
				</ui:attributes>
				<ui:attributes ox="1" oy="0" color="graph_alt_2">
					<marker v="+24 db" />
					<marker v="0 db" />
					<marker v="-24 db" />
				</ui:attributes>
				
				<!-- Meshes -->
				<mesh id="fftg_m" width="2" color="middle_channel" visibility=":fftv_m" />
				<mesh id="fftg_s" width="2" color="side_channel" visibility=":fftv_s" />
				<mesh id="ag_m" width="3" color="middle_channel" smooth="true" visibility=":fftv_m" />
				<mesh id="ag_s" width="3" color="side_channel" smooth="true" visibility=":fftv_s" />
				
